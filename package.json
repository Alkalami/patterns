{
  "name": "cob-patterns",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently \"gulp\" \"fractal start --watch\" \"stencil build --watch\"",
    "build": "stencil build && gulp build && fractal build",
    "heroku-postbuild": "npm run build",
    "lint": "eslint --ext .tsx,.js .",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --quiet",
    "*.svg": [
      "svgo --config svgo.yml",
      "git add"
    ]
  },
  "dependencies": {
    "@frctl/fractal": "^1.1.4",
    "@stencil/core": "^0.4.0",
    "@types/esri-leaflet": "^2.1.2",
    "@types/geojson": "^7946.0.1",
    "@types/jest": "^22.1.1",
    "@types/leaflet": "^1.2.5",
    "autoprefixer": "^6.3.6",
    "concurrently": "^3.5.1",
    "esri-leaflet": "^2.1.2",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-load-plugins": "^1.2.4",
    "gulp-stylus": "^2.3.1",
    "gulp-uglify": "^3.0.0",
    "jest": "^22.1.4",
    "laggard": "^0.1.0",
    "leaflet": "^1.3.1",
    "lost": "^8.1.0",
    "postcss-flexibility": "^1.1.0",
    "poststylus": "^0.2.3",
    "require-dir": "^0.3.0",
    "rucksack-css": "^0.9.1",
    "run-sequence": "^1.2.2",
    "serve-static": "^1.11.1"
  },
  "devDependencies": {
    "eslint": "^4.17.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "prettier": "^1.10.2",
    "svgo": "^1.0.4",
    "typescript": "^2.7.1",
    "typescript-eslint-parser": "^12.0.0"
  }
}
