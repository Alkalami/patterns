{{#each links as |link|}}
  <div class="lwi g-c--1-4--lg g-c--1-2--md">
    <a href="{{ link.href }}" title="{{ link.name }}" class="lwi-l">
      <span class="lwi-ic"><img src="{{ link.image }}" alt="{{ link.name }}" class="lwi-i" /></span>
      <span class="lwi-t">{{ link.name }}</span>
    </a>
  </div>
{{/each}}
